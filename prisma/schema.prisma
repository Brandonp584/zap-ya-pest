generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
}

model Lead {
  id        String     @id @default(uuid())
  name      String
  email     String
  phone     String?
  message   String
  source    String   @default("website")
  status    String   @default("NEW")
  createdAt DateTime @default(now())
}
